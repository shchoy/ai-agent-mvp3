# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0.1

# Token Management (adjust based on your model's limits)
MAX_TOKENS_PER_REQUEST=100000
MAX_TOKENS_OUTPUT=4000
CHUNK_SIZE=15000
CHUNK_OVERLAP=1000

# Pinecone Configuration
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment
PINECONE_INDEX_NAME=ai-agent-tender-response

# Business Rules
MAX_TENDER_VALUE=500000
MIN_DAYS_TO_SUBMISSION=10
SIMILARITY_THRESHOLD=0.7

# File Processing
MAX_FILE_SIZE=50000000
UPLOAD_DIR=data/uploads
SAMPLE_DIR=data/sample_tenders

# LangSmith (Optional - for debugging)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=Tender-Qualification-Agent
